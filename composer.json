{
    "name": "scaling/laravel-playbook",
    "description": "",
    "keywords": [
        "scaling",
        "laravel-playbook"
    ],
    "homepage": "https://github.com/scaling/laravel-playbook",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Erik",
            "email": "erik@scaling.studio",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.7.0|~5.8.0",
        "illuminate/console": "~5.7.0|~5.8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.7.0|~3.8.0",
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Scaling\\Playbook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scaling\\Playbook\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Scaling\\Playbook\\PlaybookServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
